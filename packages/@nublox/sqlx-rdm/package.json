{
    "name": "@nublox/sqlx-rdm",
    "version": "0.1.0",
    "description": "Runtime Driver Micro-builder for performance-optimized driver proxies",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "build": "tsup src/index.ts --format esm --dts --clean",
        "dev": "tsup src/index.ts --format esm --dts --clean --watch",
        "clean": "rm -rf dist",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@nublox/sqlx-observe": "workspace:*"
    },
    "devDependencies": {
        "tsup": "^8.0.0",
        "typescript": "^5.3.0"
    },
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public"
    }
}