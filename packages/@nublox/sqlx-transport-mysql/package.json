  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist || rm -rf dist"
  },
  "dependencies": {
    "@nublox/sqlx-core": "workspace:*"
  },
  "engines": {
    "node": ">=18"
  }
}