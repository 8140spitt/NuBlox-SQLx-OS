{
  "name": "mysql-wire-pack",
  "protocol": {
    "handshake": "v10",
    "authPlugins": [
      "mysql_native_password",
      "caching_sha2_password"
    ],
    "usesEOFDeprecated": true
  },
  "discover": {
    "version": {
      "sql": "SELECT VERSION() AS version"
    },
    "variables": [
      {
        "key": "version_comment",
        "sql": "SHOW VARIABLES LIKE 'version_comment'"
      },
      {
        "key": "sql_mode",
        "sql": "SHOW VARIABLES LIKE 'sql_mode'"
      }
    ]
  },
  "defaults": {
    "limits": {
      "maxIdentifierLen": 63,
      "maxParams": 32767
    },
    "features": {
      "ddl.concurrentIndexCreate": false
    }
  },
  "rules": {
    "featuresByVersion": [
      {
        "key": "sql.cte",
        "minVersion": "8.0.0"
      },
      {
        "key": "sql.window",
        "minVersion": "8.0.0"
      },
      {
        "key": "sql.json",
        "minVersion": "5.7.8"
      }
    ],
    "featuresByProbe": [
      {
        "key": "ddl.onlineAlter",
        "assumeTrueIf": {
          "minVersion": "8.0.12"
        }
      }
    ]
  },
  "security": {
    "tlsPolicyDefault": "requireForRemote",
    "trustDefault": "system"
  }
}